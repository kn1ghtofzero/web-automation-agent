# Google API key for Gemini model (required)
GOOGLE_API_KEY=your_api_key_here

# MakeMyTrip credentials (required for flight booking)
MMT_EMAIL=your_email@example.com
MMT_PASSWORD=your_password_here

# Chrome profile directory (optional)
# Example Windows path: C:/Users/username/AppData/Local/Google/Chrome/User Data
# Leave empty to use a fresh profile
BROWSER_USER_DATA_DIR=